  {% assign product_handles = settings.free_vimla-producthandle %}
  {% if product_handles contains product.handle %}
    <script type="text/javascript">
          window.location = "/";
    </script>
    {% endif %}

{% capture full_section %}
    {% if canonical_url contains 'vimla' or canonical_url contains 'telenor' %}
      <script>
        window.location.replace("{{ routes.root_url }}");
      </script>
    {% endif %}
{% endcapture %}

{%- if localization.country == 'Danmark' and settings.Denmark -%}
  {{ full_section }}
{% elsif localization.country == 'Sverige' and settings.Sverige %}
  {{ full_section }}
{% elsif localization.country == 'Italia' and settings.Italy %}
  {{ full_section }}
{% elsif localization.country == 'Deutschland' and settings.Germay %}
  {{ full_section }}
{% elsif localization.country == 'France' and settings.France %}
  {{ full_section }}
{% elsif localization.country == 'Espa√±a' and settings.spain %}
  {{ full_section }}
{% endif %}
{% comment %}hide vimla and telenor : end{% endcomment %}

        
  <!-- do NOT TOUCH anything below this line ================================================== -->    
  {{ content_for_header }}
  {% comment %}
