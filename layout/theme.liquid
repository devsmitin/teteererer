  {%- if template != 'cart' -%}
  {% render 'cart-drawer' %}
  {%- endif -%}
  
  <div class="PageContainer">
     
    {% if template contains 'telenorgreenroom' or template contains 'telenor-tillbehor' or collection.handle == tl_hendel or header_show %}
    {% section 'telenor-announcement' %}
    {% else %}
    {% unless template contains "vimla" %}
    {% section 'announcement' %}
    {% endunless %}
    {% endif %}

    {% if template contains'vimla' %}
    {% section 'vimla-announcementbar' %}
    {% endif %}
    {% if template contains 'vimla' %}
    {% section 'vimla-header' %}
    {% endif %}

    {% if template contains 'telenorgreenroom'  or template contains 'telenor-tillbehor' or collection.handle == tl_hendel or header_show %}
    {% section 'telenor-header' %}
    {% else %}
    {% unless template contains "vimla" %}
    {% section 'header' %}
    {% endunless %}
    {% endif %}


    <main id="main" role="main">
      {{ content_for_layout }}

    </main>

          {% if settings.enable_all_renewed %}
                  {% assign free_all_pro = settings.free_all_product_subscribe %}
                  {% for variant in free_all_pro.variants %}
                   <p id="free_allproduct" style="display:none;" value="{{ variant.id }}"></p> 
                    {% endfor %}
                {% endif %}
                
                {% if settings.enable_unsubscribe %}
                  {% assign free_vim_pro = settings.free_vimla-product %}
                  {% assign vim_pro_id = free_vim_pro.id %}
                  {% for variant in free_vim_pro.variants %}
                   <p id="free_vimlaproduct" style="display:none;" value="{{ variant.id }}"></p> 
                    {% endfor %}
                {% endif %}
                
                {% if settings.enable_subscribe %}
                   {% assign free_sub_pro = settings.free_vimla_product_subscribe %}
                  {% assign vim_sub_id = free_sub_pro.id %}
                  {% for variant in free_sub_pro.variants %}
                   <p id="free_sub_product" style="display:none;" value="{{ variant.id }}"></p> 
                    {% endfor %}
                  {% endif %}
                {% if settings.enable_discountcode %}
                <p id="auto_discount-code" style="display:none;"></p>
                {% endif %}
                

    {% section 'footer' %}
  </div>

  {% render 'microdata-schema' %} 

